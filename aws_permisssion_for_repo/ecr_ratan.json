{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowGetAuthorizationToken",
            "Effect": "Allow",
            "Action": "ecr:GetAuthorizationToken",
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "ecr:GetDownloadUrlForLayer",
                "ecr:BatchCheckLayerAvailability",
                "ecr:GetRepositoryPolicy",
                "ecr:DescribeRepositories",
                "ecr:ListImages",
                "ecr:DescribeImages",
                "ecr:BatchGetImage",
                "ecr:InitiateLayerUpload",
                "ecr:UploadLayerPart",
                "ecr:CompleteLayerUpload",
                "ecr:PutImage",
                "ecr:SetRepositoryPolicy",
                "ecr:ListTagsForResource",
                "ecr:TagResource",
                "ecr:UntagResource"
            ],
            "Resource": [
                "arn:aws:ecr:ap-south-1:421145715538:repository/ask-your-policy-ai",
                "arn:aws:ecr:ap-south-1:421145715538:repository/ratan-admin",
                "arn:aws:ecr:ap-south-1:421145715538:repository/upwork-ai"

            ]
        }
    ]
}